<body>
	<h1 style="text-align: center;">This is the home Page</h1>
	

	<ul id="unOrder">
		
	
	</ul>

</body>

	<script type="text/javascript">
	var inner="";
	// Ajax call
		
	fetch("/messageAPI").then(errFunction).then(function(res){
		return res.json();
	}).then(function(data){
		data.forEach(function(data){
			inner+="<li>"+data.name+"</li>";
			document.getElementById("unOrder").innerHTML=inner;
			console.log(inner);
		});
		
		
	}).catch(function(err){console.log(err);});
	
	function errFunction(res){
  if(!res.ok){
    throw Error(res.status);
  }
  return res;
}
	
	</script>

